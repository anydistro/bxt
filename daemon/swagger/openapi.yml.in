  /api/packages:
    get:
      tags:
        - api
        - packages
      summary: Get packages by section
      description: >-
        The endpoint makes an HTTP GET request to retrieve packages based on the
        provided branch, repository, and architecture parameters. The response
        is a JSON object with an array of package objects. Each package object
        includes the name, section, poolEntries, and preferredLocation
        properties.


        The package object has the following properties:


        - "name": (string) The name of the package.
            
        - "section": (string) The section to which the package belongs.
            
        - "poolEntries": (object) An object containing pool entries for the
        package, with each entry having a version and hasSignature property.
            - "culpa_fd": (object) An example pool entry object with version and hasSignature properties.
                - "version": (string) The version of the package.
                    
                - "hasSignature": (string) Indicates whether the package has a signature.
                    
        - "preferredLocation": (string) The preferred location for the package.
      operationId: getPackagesBySection
      parameters:
        - name: branch
          in: query
          schema:
            type: string
            example: ''
          description: '(Required) '
        - name: repository
          in: query
          schema:
            type: string
            example: ''
          description: '(Required) '
        - name: architecture
          in: query
          schema:
            type: string
            example: ''
          description: '(Required) '
      responses:
        '200':
          description: Package list / Get packages by section
          headers:
            Alt-Svc:
              schema:
                type: string
                example: h3=":443"; ma=2592000
            Content-Length:
              schema:
                type: string
                example: '413'
            Date:
              schema:
                type: string
                example: Tue, 16 Jul 2024 08:56:37 GMT
            Server:
              schema:
                type: string
                example: drogon/1.8.2
          content:
            application/json:
              schema:
                type: array
                items:
                  type: object
                  properties:
                    name:
                      type: string
                      example: <string>
                    poolEntries:
                      type: object
                      properties:
                        culpa_fd:
                          type: object
                          properties:
                            hasSignature:
                              type: string
                              example: <boolean>
                            version:
                              type: string
                              example: <string>
                        laborume3d:
                          type: object
                          properties:
                            hasSignature:
                              type: string
                              example: <boolean>
                            version:
                              type: string
                              example: <string>
                    preferredLocation:
                      type: string
                      example: <string>
                    section:
                      type: string
                      example: <string>
                example:
                  - name: <string>
                    poolEntries:
                      culpa_fd:
                        hasSignature: <boolean>
                        version: <string>
                    preferredLocation: <string>
                    section: <string>
                  - name: <string>
                    poolEntries:
                      laborume3d:
                        hasSignature: <boolean>
                        version: <string>
                    preferredLocation: <string>
                    section: <string>
              examples:
                Get packages by section:
                  value:
                    - name: abseil-cpp
                      poolEntries:
                        overlay:
                          hasSignature: true
                          version: 20240116.2-2
                      preferredLocation: overlay
                      section:
                        architecture: x86_64
                        branch: unstable
                        repository: multilib
                    - name: arch-install-scripts
                      poolEntries:
                        overlay:
                          hasSignature: true
                          version: 28-1
                      preferredLocation: overlay
                      section:
                        architecture: x86_64
                        branch: unstable
                        repository: multilib
                Package list:
                  value:
                    - name: <string>
                      poolEntries:
                        culpa_fd:
                          hasSignature: <boolean>
                          version: <string>
                      preferredLocation: <string>
                      section: <string>
                    - name: <string>
                      poolEntries:
                        laborume3d:
                          hasSignature: <boolean>
                          version: <string>
                      preferredLocation: <string>
                      section: <string>
        '400':
          description: Invalid request
          content:
            text/plain:
              examples:
                Invalid request:
                  value: ''
        '403':
          description: No permissions
          content:
            text/plain:
              examples:
                No permissions:
                  value: ''
      security:
        - bearerAuth: []
